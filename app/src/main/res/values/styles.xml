    <style name="editProfile" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="colorPrimaryDark">@color/primary_dark</item>
    </style>

    <style name="chatDetailsTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="colorPrimaryDark">@color/primary_dark</item>
    </style>

    <style name="DomainTheme" parent="Theme.AppCompat.Light.DarkActionBar">
